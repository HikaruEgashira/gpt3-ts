{
    "scripts": {
        "test": "jest",
        "dev": "esno example/main.ts",
        "build": "rimraf dist && tsup src/class.ts src/fp.ts --dts --format cjs,esm",
        "package-check": "npx @skypack/package-check"
    },
    "dependencies": {
        "dotenv": "^10.0.0",
        "got": "^11.8.2"
    },
    "devDependencies": {
        "@types/jest": "26.0.24",
        "esbuild": "^0.12.25",
        "esbuild-jest": "^0.5.0",
        "eslint": "^7.32.0",
        "esno": "^0.9.1",
        "jest": "^27.1.0",
        "rimraf": "^3.0.2",
        "tsup": "^4.14.0",
        "typescript": "^4.4.2"
    },
    "name": "@hikae/gpt",
    "version": "1.0.0",
    "description": "",
    "keywords": [
        "gpt3"
    ],
    "main": "dist/class.js",
    "module": "dist/class.mjs",
    "types": "dist/class.d.ts",
    "exports": {
        "./class": {
            "require": "./dist/class.js",
            "import": "./dist/class.mjs",
            "default": "./dist/class.mjs"
        },
        "./fp": {
            "require": "./dist/fp.js",
            "import": "./dist/fp.mjs",
            "default": "./dist/fp.mjs"
        }
    },
    "files": [
        "dist"
    ],
    "size-limit": [
        {
            "path": "dist/class.js",
            "limit": "4500 ms"
        },
        {
            "path": "dist/class.mjs",
            "limit": "4500 ms"
        }
    ],
    "author": "HikaruEgashira",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/HikaruEgashira/gpt.git"
    },
    "bugs": {
        "url": "https://github.com/HikaruEgashira/gpt/issues"
    },
    "homepage": "https://github.com/HikaruEgashira/gpt#readme"
}
